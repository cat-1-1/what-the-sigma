-- Scripts:

local function RHMJ_fake_script() -- Print.LocalScript 
	local script = Instance.new('LocalScript', Print)

	local button = script.Parent
	local textbox = button.Parent["Text Box"]
	
	button.MouseButton1Click:Connect(function()
		print(textbox.Text)
	end)
end
coroutine.wrap(RHMJ_fake_script)()
local function GQXOGAT_fake_script() -- Destroy.LocalScript 
	local script = Instance.new('LocalScript', Destroy)

	local button = script.Parent
	local gui = button.Parent.Parent
	
	button.MouseButton1Click:Connect(function()
		gui:Destroy()
	end)
end
coroutine.wrap(GQXOGAT_fake_script)()
local function MMWLCXQ_fake_script() -- Main.SmoothDrag 
	local script = Instance.new('LocalScript', Main)

	local Drag = script.Parent.Parent.Main
	gsCoreGui = game:GetService("CoreGui")
	gsTween = game:GetService("TweenService")
	local UserInputService = game:GetService("UserInputService")
		local dragging
		local dragInput
		local dragStart
		local startPos
		local function update(input)
			local delta = input.Position - dragStart
			local dragTime = 0.04
			local SmoothDrag = {}
			SmoothDrag.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
			local dragSmoothFunction = gsTween:Create(Drag, TweenInfo.new(dragTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), SmoothDrag)
			dragSmoothFunction:Play()
		end
		Drag.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				dragging = true
				dragStart = input.Position
				startPos = Drag.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)
		Drag.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		UserInputService.InputChanged:Connect(function(input)
			if input == dragInput and dragging and Drag.Size then
				update(input)
			end
		end)
	
end
coroutine.wrap(MMWLCXQ_fake_script)()
local function TPWMHJA_fake_script() -- Clear.LocalScript 
	local script = Instance.new('LocalScript', Clear)

	local button = script.Parent
	local box = button.Parent["Text Box"]
	
	button.MouseButton1Click:Connect(function()
		box.Text = ''
	end)
end
coroutine.wrap(TPWMHJA_fake_script)()
local function JVWNAJ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local button = script.Parent
	local frame = button.Parent
	
	button.MouseButton1Click:Connect(function()
		frame:Destroy()
	end)
end
coroutine.wrap(JVWNAJ_fake_script)()
local function GKMQT_fake_script() -- BRODIDNOTCOOKWITHTHISONE.LocalScript 
	local script = Instance.new('LocalScript', BRODIDNOTCOOKWITHTHISONE)

	local gui = script.Parent
	
	
	gui.Text.Visible = true
	task.wait(5)
	gui.Text.Visible = false
	
end
coroutine.wrap(GKMQT_fake_script)()
local function QOND_fake_script() -- BRODIDNOTCOOKWITHTHISONE.LocalScript 
	local script = Instance.new('LocalScript', BRODIDNOTCOOKWITHTHISONE)

	local UserInputService = game:GetService("UserInputService")
	
	function onKeyPress(inputObject, gameProcessedEvent)
		if not gameProcessedEvent then
			if inputObject.KeyCode == Enum.KeyCode.RightControl then 
				if script.Parent.Main.Visible == false then
					script.Parent.Main.Visible = true
				else
					script.Parent.Main.Visible = false
				end
			end
		end
	end
	
	UserInputService.InputBegan:connect(onKeyPress)
end
coroutine.wrap(QOND_fake_script)()
